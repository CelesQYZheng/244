import java.io.IOException;

public class Program1{
    public static void main(String[] args){
        byte[]data = {1,3,4,5};
        try{
            System.in.read(data,0,data.length);
            System.out.write(data);
        }catch(IOException e){
            System.err.println("I/O error occurs");
        }

    }
}
